{
  "name": "place-puppy",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "babel src/server --out-dir dist && cp src/server/images.json package.json dist && npm install --only=production --prefix dist",
    "clean": "rm -rf dist",
    "start": "AWS_PROFILE=place-puppy PUBLIC_PATH=dist/public babel-watch ./src/server/server.js"
  },
  "author": "AJ Rabe <aj.rabe@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/seibar/place-puppy.git",
  "dependencies": {
    "aws-sdk": "2.154.0",
    "express": "4.16.2",
    "jimp": "0.2.28",
    "node-fetch": "1.7.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "uglify-js-es6": "2.8.9",
    "uglifyes-webpack-plugin": "0.4.3",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.5"
  }
}
